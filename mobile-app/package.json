{
  "name": "zebra-iota-edge-sdk",
  "version": "0.1.0",
  "scripts": {
    "build": "run-p build cap-run:android",
    "autobuild": "rollup -c -w",
    "local": "run-p start:dev autobuild",
    "start": "sirv public --single",
    "start:dev": "sirv public --single --dev",
    "serve:capacitor": "sirv public --single --dev --host --port 5001",
    "cap-run:android": "cap sync android && cap open android",
    "dev:android": "run-p serve:capacitor autobuild cap-run:android"
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/preset-env": "^7.14.2",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-typescript": "^8.2.1",
    "@rollup/plugin-wasm": "^5.1.2",
    "@tsconfig/svelte": "^1.0.10",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.24.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.26.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.23.2",
    "eslint-plugin-prettier": "^3.4.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "prettier-plugin-svelte": "^2.3.0",
    "rollup": "^2.46.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "svelte": "^3.38.0",
    "svelte-check": "^1.5.4",
    "svelte-loader": "^3.1.1",
    "svelte-preprocess": "^4.7.2",
    "ts-loader": "^9.2.1",
    "tslib": "^2.2.0",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "@capacitor/android": "^2.4.7",
    "@capacitor/cli": "^2.4.7",
    "@capacitor/core": "^2.4.7",
    "@iota/identity-wasm": "^0.3.1",
    "bwip-js": "^3.0.0",
    "capacitor-secure-storage-plugin": "^0.5.0",
